<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/custom_start_appbar_cl"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:elevation="1dp"
        android:background="@color/white"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent">
        <ImageButton
            android:id="@+id/custom_start_back_btn"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:src="@drawable/ic_navigate_prev"
            android:background="@color/transparent"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="6dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"/>
        <TextView
            android:id="@+id/custom_start_appbar_title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="운동/코스"
            android:fontFamily="@font/pretendard_2"
            android:textSize="16sp"
            android:textColor="@color/gray_950"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/custom_start_appbar_cl"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:paddingHorizontal="25dp">
        <TextView
            android:id="@+id/custom_start_title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="어떤 운동을 찾고싶으신가요?"
            android:fontFamily="@font/pretendard_2"
            android:textSize="26sp"
            android:textColor="@color/gray_950"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginTop="12dp"/>
        <TextView
            android:id="@+id/custom_start_subtitle_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="찾고싶은 운동을 검색해보세요!"
            android:fontFamily="@font/pretendard"
            android:textSize="13sp"
            android:textColor="@color/gray_700"
            app:layout_constraintTop_toBottomOf="@id/custom_start_title_tv"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginTop="13dp"/>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/custom_start_search_cl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/custom_start_subtitle_tv"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="12dp">
            <EditText
                android:id="@+id/custom_start_search_et"
                android:layout_width="match_parent"
                android:layout_height="36dp"
                android:background="@drawable/et_gray200_border43"
                android:inputType="text"
                android:imeOptions="actionSearch"
                android:maxLines="1"
                android:hint="운동명 / 자세 / 부위를 입력해주세요"
                android:fontFamily="@font/pretendard"
                android:textSize="15sp"
                android:textColorHint="@color/gray_700"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:paddingLeft="50dp"/>
            <ImageView
                android:id="@+id/custom_start_search_ic_iv"
                android:layout_width="28dp"
                android:layout_height="28dp"
                android:src="@drawable/ic_search_gray700"
                app:layout_constraintTop_toTopOf="@id/custom_start_search_cl"
                app:layout_constraintBottom_toBottomOf="@id/custom_start_search_cl"
                app:layout_constraintLeft_toLeftOf="@id/custom_start_search_cl"
                android:layout_marginLeft="18dp"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <FrameLayout
            android:id="@+id/search_type_frm"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@id/custom_start_search_cl"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>