<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/setting_appbar_cl"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:background="@color/white"
        android:elevation="1dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/setting_back_btn"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="5dp"
            android:background="@color/transparent"
            android:src="@drawable/ic_navigate_prev"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/setting_appbar_title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/pretendard_2"
            android:text="설정"
            android:textColor="@color/gray_950"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/setting_notification_title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="알림설정"
        android:fontFamily="@font/pretendard_2"
        android:textColor="@color/gray_950"
        android:textSize="13sp"
        app:layout_constraintTop_toBottomOf="@id/setting_appbar_cl"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="14dp"
        android:layout_marginLeft="34dp"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/setting_notification_btn"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        app:layout_constraintTop_toBottomOf="@id/setting_notification_title_tv"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:paddingHorizontal="30dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp">
        <TextView
            android:id="@+id/setting_notification_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="찜한 운동 팝업창"
            android:fontFamily="@font/pretendard"
            android:textColor="@color/gray_950"
            android:textSize="15sp"
            app:layout_constraintTop_toTopOf="@id/setting_notification_btn"
            app:layout_constraintBottom_toBottomOf="@id/setting_notification_btn"
            app:layout_constraintLeft_toLeftOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <View
        android:id="@+id/div1_v"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/gray_200"
        app:layout_constraintTop_toBottomOf="@id/setting_notification_btn"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

    <TextView
        android:id="@+id/setting_user_title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="사용자 설정"
        android:fontFamily="@font/pretendard_2"
        android:textColor="@color/gray_950"
        android:textSize="13sp"
        app:layout_constraintTop_toBottomOf="@id/setting_notification_btn"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="34dp"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/setting_user_btn"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        app:layout_constraintTop_toBottomOf="@id/setting_user_title_tv"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:paddingHorizontal="30dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp">
        <TextView
            android:id="@+id/setting_user_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="계정 및 정보 관리"
            android:fontFamily="@font/pretendard"
            android:textColor="@color/gray_950"
            android:textSize="15sp"
            app:layout_constraintTop_toTopOf="@id/setting_user_btn"
            app:layout_constraintBottom_toBottomOf="@id/setting_user_btn"
            app:layout_constraintLeft_toLeftOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <View
        android:id="@+id/div2_v"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/gray_200"
        app:layout_constraintTop_toBottomOf="@id/setting_notification_btn"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

    <TextView
        android:id="@+id/setting_etc_title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="기타"
        android:fontFamily="@font/pretendard_2"
        android:textColor="@color/gray_950"
        android:textSize="13sp"
        app:layout_constraintTop_toBottomOf="@id/setting_user_btn"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="34dp"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/setting_logout_btn"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        app:layout_constraintTop_toBottomOf="@id/setting_etc_title_tv"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:paddingHorizontal="30dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp">
        <TextView
            android:id="@+id/setting_logout_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="로그아웃"
            android:fontFamily="@font/pretendard"
            android:textColor="@color/gray_950"
            android:textSize="15sp"
            app:layout_constraintTop_toTopOf="@id/setting_logout_btn"
            app:layout_constraintBottom_toBottomOf="@id/setting_logout_btn"
            app:layout_constraintLeft_toLeftOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <View
        android:id="@+id/div3_v"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/gray_200"
        app:layout_constraintTop_toBottomOf="@id/setting_logout_btn"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/setting_withdrawal_btn"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        app:layout_constraintTop_toBottomOf="@id/setting_logout_btn"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:paddingHorizontal="30dp"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp">
        <TextView
            android:id="@+id/setting_withdrawal_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="탈퇴하기"
            android:fontFamily="@font/pretendard"
            android:textColor="@color/gray_950"
            android:textSize="15sp"
            app:layout_constraintTop_toTopOf="@id/setting_withdrawal_btn"
            app:layout_constraintBottom_toBottomOf="@id/setting_withdrawal_btn"
            app:layout_constraintLeft_toLeftOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <View
        android:id="@+id/div4_v"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/gray_200"
        app:layout_constraintTop_toBottomOf="@id/setting_withdrawal_btn"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>